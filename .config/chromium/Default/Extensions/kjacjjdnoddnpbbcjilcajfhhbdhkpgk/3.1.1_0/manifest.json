{
   "author": "ForestApp Team",
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/icon-512.png",
      "default_popup": "panel.html"
   },
   "content_scripts": [ {
      "css": [ "stylesheets/overlay.css" ],
      "js": [ "js/overlay.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "default_locale": "en",
   "description": "Stay focused in a pleasant way.",
   "homepage_url": "http://www.forestapp.cc",
   "icons": {
      "128": "icons/icon-512.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Forest: Stay focused, be present",
   "options_page": "settings.html",
   "permissions": [ "activeTab", "storage", "notifications", "tabs", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.1.1",
   "web_accessible_resources": [ "images/*.png", "images/*.svg", "fonts/*.ttf" ]
}
